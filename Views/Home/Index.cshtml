<style>
    .layout-footer {
        display: none;

    }

    .index-title {
        font-family: 'Covered By Your Grace', cursive;
        font-size: 5rem;

    }


    .page-title .row {
        position: fixed;
        font-size: 100px;
        color: white;
        left: 0;
        right: 0;
